<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="341dp"
        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="397dp"
            android:layout_height="295dp">

            <TextView
                android:id="@+id/ps"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"

                android:text="Project Name :" />


            <TextView
                android:id="@+id/prg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg"
                android:layout_marginStart="1dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg6"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg3"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg4"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg8"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:focusableInTouchMode="false"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/prg8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg5"
                android:layout_marginStart="0dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/parts"
                android:focusableInTouchMode="false"
                android:paddingStart="10dp"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/parts"
                android:layout_width="129dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ps"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="Customer Name :" />

            <TextView
                android:id="@+id/qty"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/parts"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="Contact :" />


            <TextView
                android:id="@+id/am"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/qty"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="Address :" />

            <TextView
                android:id="@+id/chalan"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/am"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="Email :" />

            <TextView
                android:id="@+id/startdate"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/chalan"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="Start Date :" />

            <TextView
                android:id="@+id/startdate2"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/startdate"
                android:layout_marginTop="10dp"
                android:paddingStart="10dp"
                android:paddingLeft="10dp"
                android:text="End Date :" />


            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/prg7"
                android:layout_alignParentRight="true"
                android:layout_marginTop="20dp"
                android:layout_marginRight="128dp"
                android:clickable="true"
                android:elevation="16dp"
                android:focusable="true"
                android:src="@drawable/ic_add"
                android:visibility="visible"
                tools:ignore="VectorDrawableCompat" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/edit"
                android:layout_width="wrap_content"
                android:layout_height="71dp"
                android:layout_below="@+id/prg7"
                android:layout_alignParentRight="true"
                android:layout_marginTop="20dp"
                android:layout_marginRight="196dp"
                android:clickable="true"
                android:elevation="16dp"
                android:focusable="true"
                android:src="@drawable/ic_edit"
                android:visibility="visible"
                tools:ignore="VectorDrawableCompat" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/pdf"
                android:layout_width="wrap_content"
                android:layout_height="71dp"
                android:layout_below="@+id/prg7"
                android:layout_alignParentRight="true"
                android:layout_marginTop="20dp"
                android:layout_marginRight="269dp"
                android:clickable="true"
                android:elevation="16dp"
                android:focusable="true"
                android:src="@drawable/pdf"
                android:visibility="visible"
                tools:ignore="VectorDrawableCompat" />

        </RelativeLayout>

    </androidx.cardview.widget.CardView>


</RelativeLayout>